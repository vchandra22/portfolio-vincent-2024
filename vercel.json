{
    "version": 2,
    "framework": null,
    "public": "public",
    "rewrites": [
        { "source": "/js/(.*)", "destination": "/public/js/$1" },
        { "source": "/css/(.*)", "destination": "/public/css/$1" },
        { "source": "/images/(.*)", "destination": "/public/images/$1" },
        { "source": "/(.*)", "destination": "/index.php" }
    ],
    "functions": {
        "api/index.php": { "runtime": "vercel-php@0.6.0" }
    },
    "routes": [
        {
            "src": "/img/(.*)",
            "dest": "public/img/(.*)"
        },
        {
            "src": "/(.*)",
            "dest": "/api/index.php"
        }
    ],
    "env": {
        "APP_ENV": "production",
        "APP_DEBUG": "true",
